{
  "audio_file": "input.mp3",
  "output_file": "flower_output.mp4",
  "visual": {
    "type": "frequency_flower",
    "resolution": [1920, 1080],
    "fps": 60,
    "colors": {
      "primary": "#FFB7B2",
      "secondary": "#FFDAC1",
      "background": "#121015"
    },
    "params": {
      "petal_layers": 3,
      "pollen_effect": true,
      "stem_growth": true
    }
  },
  "postprocess": {
    "contrast": 1.05,
    "saturation": 1.2,
    "grain": 0.03,
    "vignette": 0.25
  }
}
